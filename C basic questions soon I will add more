ans17. 
#include<stdio.h>

int main(){
    int marks[2];
    printf("Enter Math,Englih,Computer subjects marks: "); scanf("%d %d %d",&marks[0],&marks[1],&marks[2]);
    printf("Your Math marks is: %d \nYour English marks is: %d \nYour Computer marks is: %d",marks[0],marks[1],marks[2]); 
}
